<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <children>
      <ComboBox fx:id="chooseClass_cb" layoutX="130.0" layoutY="37.0" onAction="#chooseClass_cb_onAction" prefWidth="150.0" />
      <Label layoutX="50.0" layoutY="41.0" text="Choose class:" />
      <Label layoutX="345.0" layoutY="98.0" text="Choose object:" />
      <ComboBox fx:id="chooseObject_cb" layoutX="436.0" layoutY="94.0" onAction="#chooseObject_cb_onAction" prefWidth="150.0" />
      <TableView fx:id="tableView" layoutX="50.0" layoutY="94.0" prefHeight="242.0" prefWidth="279.0">
        <columns>
          <TableColumn fx:id="tableColumn1" prefWidth="47.0" text="No." />
          <TableColumn fx:id="tableColumn2" prefWidth="124.0" text="Field" />
            <TableColumn fx:id="tableColumn3" prefWidth="107.0" text="Value" />
        </columns>
      </TableView>
      <Button fx:id="createObject_bt" layoutX="345.0" layoutY="165.0" mnemonicParsing="false" onAction="#createObjectOnAction" text="Create a new object" />
      <Button fx:id="deleteObject_bt" layoutX="345.0" layoutY="215.0" mnemonicParsing="false" onAction="#deleteObjectOnAction" prefHeight="25.0" prefWidth="121.0" text="Delete object" />
      <TextField fx:id="setValue_tf" layoutX="345.0" layoutY="311.0" prefHeight="25.0" prefWidth="104.0" />
      <Button fx:id="setValue_bt" layoutX="468.0" layoutY="311.0" mnemonicParsing="false" onAction="#setValue_bt_OnAction" prefHeight="25.0" prefWidth="87.0" text="Set value" />
   </children>
</AnchorPane>
